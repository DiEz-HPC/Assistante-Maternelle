{% block stylesheets %}
    {{ encore_entry_link_tags('swiper') }}
{% endblock %}

<div
    class="swiper-container">
    <!-- swiper slides -->
    <div class="swiper-wrapper">
        <div class="swiper-slide" style="background-image: url(https://source.unsplash.com/random?sig=24);"></div>

        <div class="swiper-slide" style="background-image: url(https://source.unsplash.com/random?sig=53);"></div>

        <div class="swiper-slide" style="background-image: url(https://source.unsplash.com/random?sig=52);"></div>

        <div class="swiper-slide" style="background-image: url(https://source.unsplash.com/random?sig=21);"></div>

        <div class="swiper-slide" style="background-image: url(https://source.unsplash.com/random?sig=53);"></div>
    </div>
    <!-- !swiper slides -->

    <!-- next / prev arrows -->
    <div class="swiper-button-next"></div>
    <div class="swiper-button-prev"></div>
    <!-- !next / prev arrows -->

    <!-- pagination dots -->
    <div class="swiper-pagination"></div>
    <!-- !pagination dots -->
</div>
